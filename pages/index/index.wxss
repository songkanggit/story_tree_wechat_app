.classbox {
  width: 100%;
  height: 84rpx;
  padding: 36rpx 0rpx 40rpx;
  margin: 0rpx auto 20rpx;
  box-shadow: 0rpx 3rpx 10rpx 0rpx #e1e1e1;
  display: flex;
}

.classbox .classlist {
  width: 20%;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.classbox .classimg {
  width: 100%;
  height: 52rpx;
  margin-bottom: 10rpx;
}

.classimg image {
  display: inherit;
  width: 62rpx;
  height: 52rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  margin: auto auto;
}

.classbox .classtext {
  width: 100%;
  overflow: hidden;
  font-size: 22rpx;
  text-align: center;
  color: #333;
}

.indexlist {
  width: 100%;
}

.titlebox {
  width: 100%;
  height: 132rpx;
  position: relative;
}

.title {
  width: 100%;
  height: 78rpx;
  font-size: 36rpx;
  line-height: 80rpx;
  color: #333;
  text-align: center;
}

.dir {
  width: 100%;
  height: 54rpx;
  font-size: 28rpx;
  color: #999;
  text-align: center;
}

.more {
  width: 56rpx;
  padding-right: 24rpx;
  height: 26rpx;
  font-weight: bold;
  font-size: 26rpx;
  line-height: 26rpx;
  color: #ff1f43;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 20rpx;
  margin: auto auto;
}

.more image {
  width: 14rpx;
  height: 26rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto auto;
}

.list-box {
  width: 750rpx;
  margin: 0rpx auto;
}

.list {
  width: 370rpx;
  margin-right: 10rpx;
  float: left;
}

.list:nth-child(2n) {
  margin-right: 0rpx;
}

.list-img {
  width: 100%;
  height: 370rpx;
  overflow: hidden;
  border-radius: 6rpx;
  box-shadow: 0rpx 0rpx 20rpx 3rpx #e1e1e1;
  position: relative;
}

.list-img image {
  width: 98%;
  height: 98%;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto auto;
}

.list-text {
  width: 100%;
  height: 86rpx;
  font-size: 28rpx;
  line-height: 70rpx;
  text-indent: 20rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
