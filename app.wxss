/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.playbtn {
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
  overflow: hidden;
  position: fixed;
  bottom: 30rpx;
  right: 30rpx;
  z-index: 999;
}

.playbtn image {
  width: 100%;
  height: 100%;
}

button::after {
  border: none;
}
